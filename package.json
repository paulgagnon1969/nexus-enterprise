  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "turbo dev",
    "dev:api": "turbo dev --filter=api",
    "dev:all": "turbo dev --parallel",
    "dev:clean": "bash ./scripts/dev-clean.sh",
