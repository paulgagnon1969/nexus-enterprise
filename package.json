{
  "name": "nexus-enterprise",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "turbo dev",
    "dev:api": "turbo dev --filter=api",
    "dev:web": "turbo dev --filter=web",
    "dev:contact-sync": "npm --prefix apps/contact-sync run tauri:dev",
    "build:contact-sync": "npm --prefix apps/contact-sync run tauri:build",
    "dev:all": "turbo dev --parallel",
    "dev:clean": "bash ./scripts/dev-clean.sh",
    "build": "turbo build",
    "lint": "turbo lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write .",
    "db:migrate:dev": "bash ./scripts/db-migrate-cloud.sh",
    "db:migrate:prod": "bash ./scripts/db-migrate-prod.sh",
    "deploy:prod:api": "bash ./deploy-api-prod.sh",
    "deploy:prod:api:env": "bash ./scripts/deploy-api-prod-env.sh"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "packageManager": "npm@10.0.0",
  "devDependencies": {
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-fastify": "^11.1.9",
    "messagebird": "^4.0.1",
    "punycode": "^2.3.1"
  },
  "overrides": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.1",
    "@react-native/virtualized-lists": "0.83.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
