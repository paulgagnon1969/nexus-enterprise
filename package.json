  "packageManager": "npm@10.0.0",
  "devDependencies": {
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-fastify": "^11.1.9",
    "messagebird": "^4.0.1"
