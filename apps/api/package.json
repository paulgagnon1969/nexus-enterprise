{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/main.ts",
    "worker:dev": "nodemon --watch src --ext ts --exec ts-node src/worker.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "worker": "node dist/worker.js",
    "email-poller:dev": "ts-node src/email-inbound-poller.ts",
    "email-poller": "node dist/email-inbound-poller.js",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'lint disabled'"
  },
  "dependencies": {
    "@fastify/multipart": "^9.0.0",
    "@fastify/static": "^8.3.0",
    "@google-cloud/storage": "^7.18.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-fastify": "^11.0.0",
    "@prisma/client": "^7.0.0",
    "@repo/database": "0.0.1",
    "argon2": "^0.41.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.58.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^5.0.0",
    "imapflow": "^1.2.4",
    "ioredis": "^5.4.0",
    "mailparser": "^3.9.1",
    "messagebird": "^4.0.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^6.4.17",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  }
}
