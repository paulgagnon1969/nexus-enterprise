{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "dev": "npm run build:deps && expo start",
    "start": "npm run dev",
    "android": "npm run build:deps && expo start --android",
    "ios": "npm run build:deps && expo start --ios",
    "web": "npm run build:deps && expo start --web",
    "build:deps": "npm --workspace @repo/types run build",
    "check-types": "tsc --noEmit",
    "build:android": "./scripts/build-android.sh preview",
    "build:android:prod": "./scripts/build-android.sh production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/virtualized-lists": "0.81.5",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@repo/types": "0.0.1",
    "expo": "~54.0.32",
    "expo-asset": "~12.0.12",
    "expo-camera": "^17.0.10",
    "expo-file-system": "~19.0.15",
    "expo-image-manipulator": "^14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-print": "^15.0.8",
    "expo-secure-store": "~15.0.4",
    "expo-sharing": "^14.0.8",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.23.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  }
}
