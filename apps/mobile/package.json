{
  "name": "mobile",
  "version": "1.1.0-dev",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "dev": "npm run build:deps && expo start",
    "start": "npm run dev",
    "android": "npm run build:deps && expo start --android",
    "ios": "npm run build:deps && expo start --ios",
    "web": "npm run build:deps && expo start --web",
    "build:deps": "npm --workspace @repo/types run build",
    "check-types": "tsc --noEmit",
    "build:android": "npx expo run:android --variant debug",
    "build:android:release": "./scripts/build-android-local.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native/virtualized-lists": "0.83.1",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@repo/types": "0.0.1",
    "expo": "^55.0.0-preview.10",
    "expo-asset": "~55.0.4",
    "expo-camera": "~55.0.5",
    "expo-dev-client": "~55.0.5",
    "expo-file-system": "~55.0.5",
    "expo-image-manipulator": "~55.0.5",
    "expo-image-picker": "~55.0.5",
    "expo-local-authentication": "~55.0.5",
    "expo-print": "~55.0.5",
    "expo-secure-store": "~55.0.5",
    "expo-sharing": "~55.0.6",
    "expo-sqlite": "~55.0.5",
    "expo-status-bar": "~55.0.2",
    "expo-updates": "~55.0.7",
    "punycode": "^2.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.0",
    "react-native": "0.83.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.22.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "typescript": "~5.9.2"
  }
}
